{
  "presets": ["next/babel"],
  "plugins": [
    [
      "styled-components",
      {
        "ssr": true
      }
    ],
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@pages": "/pages",
          "@theme/*": "./theme/*",
          "@components": "./components",
          "@services": "./services",
          "@config": "./config"
        }
      }
    ]
  ]
}
